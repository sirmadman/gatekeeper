{
    "name":"sirmadman/gatekeeper",
    "type":"library",
    "description":"A simple PHP authentication and authorization library",
    "keywords":["authentication", "authorization"],
    "homepage":"https://github.com/sirmadman/gatekeeper.git",
    "license":"MIT",
    "authors":[
        {
            "name":"Chris Cornutt",
            "email":"ccornutt@phpdeveloper.org",
            "homepage":"http://www.phpdeveloper.org/"
        },
        {
            "name":"Thomas HÃ¤gglund",
            "email":"thomas.hgglund@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/sirmadman/modler"
        }
    ],
    "require":{
        "php":">=8.1.0",
        "sirmadman/modler": "^2.2.2",
        "robmorgan/phinx": ">=0.16.10",
        "ircmaxell/random-lib": "^1.2.0",
        "vlucas/phpdotenv": ">=5.6.2",
        "symfony/expression-language": "^7.3.2",
        "monolog/monolog": "^2.9 | ^3.9"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.5.33"
    },
    "autoload": {
        "psr-4": {
            "Psecio\\": "src/Psecio/"
        },
        "psr-0": {
            "Psecio": "src/"
        }
    },
    "bin": ["bin/setup.sh"],
    "scripts": {
        "test": "phpunit tests",
        "phpcs": [
            "phpcs --standard=PSR12 ./src"
        ],
        "phpcbf": [
            "phpcbf --standard=PSR12 ./src"
        ]
    }
}
